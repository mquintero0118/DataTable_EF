{
  "name": "@enviaflores/vue-ef-bootstrap",
  "version": "0.1.5",
  "private": false,
  "description": "A Vue.js component library for Bootstrap",
  "bugs": "https://github.com/enviaflores/vue-ef-bootstrap/issues",
  "author": "Envia Flores <development@enviaflores.com> (https://github.com/enviaflores)",
  "engines": {
    "node": ">=14.18.1",
    "npm": ">=6.14.15"
  },
  "keywords": [
    "vue",
    "vue 3",
    "template",
    "bootstrap",
    "bootstrap 5",
    "vue-ef-bootstrap",
    "envia flores"
  ],
  "contributors": [
    "Victor Corzo <victor@enviaflores.com> (https://github.com/orgs/enviaflores/people/jvjc)",
    "Edgardo Acosta <edgardo.acosta@enviaflores.com>(https://github.com/orgs/enviaflores/people/EdgardoAcosta)"
  ],
  "man": "https://github.com/enviaflores/vue-ef-bootstrap/wiki",
  "repository": {
    "type": "git",
    "url": "https://github.com/enviaflores/vue-ef-bootstrap"
  },
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js",
    "build": "vue-cli-service build ./example/main.js",
    "lint": "vue-cli-service lint",
    "docs": "vue-docgen -c config/docgen.config.js",
    "start": "vue-cli-service serve --open",
    "build:lib": "vue-cli-service build --target lib --name vue-ef ./src/index.js --mode production"
  },
  "main": "./dist/vue-ef.common.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/vue-ef.common.js"
    },
    "./dist/style.css": "./dist/vue-ef.css"
  },
  "unpkg": "dist/vue-ef.common.js",
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.1",
    "core-js": "^3.6.5",
    "datatables.net": "^1.10.25",
    "datatables.net-bs4": "^1.10.25",
    "daterangepicker": "^3.1.0",
    "highlight.js": "^10.7.3",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "mitt": "^2.1.0",
    "moment": "^2.29.1",
    "multiselect-two-sides": "^2.5.7",
    "select2": "^4.1.0-rc.0",
    "select2-bootstrap-5-theme": "^1.1.1",
    "vue": "^3.0.0",
    "vue-loader": "^15.9.8",
    "vue-router": "^4.0.0-0",
    "vue-sweetalert2": "^4.3.1",
    "vue-toastification": "^2.0.0-rc.1",
    "vue3-table-lite": "^1.1.3",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "axios": "^0.24.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.10.0",
    "sass": "1.32.*",
    "sass-loader": "^8.0.2",
    "vue-docgen-api": "^4.40.0",
    "vue-docgen-cli": "^4.40.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/eslint-config-airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "vue/no-multiple-template-root": "off",
      "indent": [
        "error",
        2
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
